<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">
    <description>101wiki semantic endpoint</description>
    <display-name>SemanticEndpoint</display-name>
    <servlet>
        <servlet-name>SemanticEndpoint</servlet-name>
        <servlet-class>org.softlang.SemanticEndpoint</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SemanticEndpoint</servlet-name>
        <url-pattern>/doQuery</url-pattern>
    </servlet-mapping>
</web-app>
